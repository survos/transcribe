{% extends 'base.html.twig' %}

{% block title %}Media {{ media.filename }}!{% endblock %}

{% block body %}

    {% for sentence in media.transcript %}
        {% for a in sentence.alternatives %}
            <b>{{ a.transcript }} <i>{{ (a.confidence*100)|number_format }}</i></b><br />
    {% endfor %}
    {% endfor %}


{% endblock %}